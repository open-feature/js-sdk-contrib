{
  "flags": {
    "fibAlgo": {
      "variants": {
        "recursive": "recursive",
        "binet": "binet"
      },
      "defaultVariant": "recursive",
      "state": "ENABLED",
      "targeting": {
        "if": [
          {
            "$ref": "emailSuffixFaas"
          },
          "binet",
          null
        ]
      }
    }
  },
  "$evaluators": {
    "emailSuffixFaas": {
      "in": [
        "@faas.com",
        {
          "var": [
            "email"
          ]
        }
      ]
    }
  }
}
